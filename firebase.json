{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "panda-api",
          "region": "australia-southeast2"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
